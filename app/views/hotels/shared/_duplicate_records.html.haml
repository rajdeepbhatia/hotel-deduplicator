.row
  .col-md-7
    = duplicate.send("#{duplicate_source}_hotel").name + ', ' + duplicate.send("#{duplicate_source}_hotel").locality
  .col-md-5.mark-duplicate-link
    - if duplicate.is_duplicated?
      Duplicate
    - else
      = link_to 'Mark Duplicate', '#', data: { id: duplicate.id }, class: 'mark-dup'